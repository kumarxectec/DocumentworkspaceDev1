<?xml version="1.0"?>
<configuration>
 <system.webServer>
 <httpErrors errorMode="Detailed" />
     <handlers>
      <!-- Route all requests to server.js using IISNode -->
      <add name="iisnode" path="server.js" verb="*" modules="iisnode" />
    </handlers>
 <rewrite>
 <rules>
    <rule name="DynamicContent">
<match url="/*" />
<action type="Rewrite" url="server.js" />
</rule>

 </rules>
 </rewrite>
 </system.webServer>
</configuration>
